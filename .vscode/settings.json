{
    "css.validate": false,
	"less.validate": false,
	"scss.validate": false,
	"stylelint.validate": ["css", "scss"],
    "files.eol": "\n",
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true,
		"source.fixAll.stylelint": true
	},
    
    "cSpell.words": [
		"clsx",
		"googleusercontent",
		"keyframes",
		"Lato",
		"nextauth",
		"nivo",
		"stylelint",
		"svgr"
	]
}